{
    "data" : {
        "url": "data/synthetic",
        "format": "typedArray",
        "schema": {
            "BirthMonth"   : "int",
            "BabyGender"   : "string",
            "BabyWeight"   : "float",
            "MotherAge"    : "int",
            "MotherRace"   : "string",
            "MotherStatus" : "string",
            "MotherEdu"    : "string",
            "MotherHeight" : "float",
            "MotherWeight" : "float",
            "MotherWgtGain": "float",
            "FatherAge"    : "int",
            "FatherRace"   : "string",
            "FatherEdu"    : "string"
        }
    },
    "pipeline": [
        {
            "$visualize": {
                "id": "chart",
                "mark": "line",
                "y": ["BabyWeight", "MotherHeight", "MotherWeight", "MotherWgtGain"],
                "color": "red",
                "perceptual": 1
            }
        }
    ]
}
