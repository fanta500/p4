{
    "data" : {
        "url": "data/synthetic",
        "dtype": "typedArray",
        "schema": {
            "BirthMonth"   : "int",
            "BabyGender"   : "string",
            "BabyWeight"   : "float",
            "MotherAge"    : "int",
            "MotherRace"   : "string",
            "MotherStatus" : "string",
            "MotherEdu"    : "string",
            "MotherHeight" : "float",
            "MotherWeight" : "float",
            "MotherWgtGain": "float"
        }
    },
    "pipeline": [
        {
            "$select": {
                "BabyWeight": [0, 15],
                "MotherWeight": [70, 370]
            }
        },
        {
            "$visualize": {
                "mark": "circle",
                "size": 10,
                "color": "red",
                "opacity": "auto",
                "x": "MotherWeight",
                "y": "BabyWeight"
            }
        }
    ]
}
