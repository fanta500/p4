{
    "data" : {
        "url": "data/synthetic",
        "dtype": "typedArray",
        "schema": {
            "BirthMonth"   : "int",
            "BabyGender"   : "string",
            "BabyWeight"   : "float",
            "MotherAge"    : "int",
            "MotherRace"   : "string",
            "MotherStatus" : "string",
            "MotherEdu"    : "string",
            "MotherHeight" : "float",
            "MotherWeight" : "float",
            "MotherWgtGain": "float",
            "FatherAge"    : "int",
            "FatherRace"   : "string",
            "FatherEdu"    : "string"
        }
    },
    "pipeline": [
        {
            "$select": {
                "BabyWeight": [0, 18],
                "MotherWeight": [70, 380]
            }
        },
        {
            "$visualize": {
                "id": "chart",
                "mark": "dot",
                "color": "red",
                "x": "MotherWeight",
                "y": "BabyWeight",
                "perceptual": 1
            }
        }
    ]
}
